		<clipnode clip="Bdies" id="25" name="Bdies"/>
		<clipnode clip="youwin" id="26" name="youwin"/>
		<clipnode clip="YT_1" id="27" name="YT_1"/>
		<clipnode clip="YT_2" id="28" name="YT_2"/>
